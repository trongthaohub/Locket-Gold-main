{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/web/$1"
        }
    ],
    "functions": {
        "api/*.py": {
            "maxDuration": 60
        }
    }
}